{
  "expo": {
    "name": "Cigua Inventory Mobile",
    "slug": "cigua-inventory-mobile",
    "version": "1.0.0",
    "sdkVersion": "52.0.0",
    "scheme": "ciguainv",
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.ciguainv.mobile"
    },
    "android": {
      "package": "com.ciguainv.mobile",
      "usesCleartextTraffic": true
    },
    "plugins": [
      "expo-router",
      "expo-sqlite"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6eb59aa0-c062-4e7e-800e-535cc262553a"
      }
    }
  }
}